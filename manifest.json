{
    "manifest_version": 2,

    "name": "8tracks SFW",
    "description": "Hides playlist pictures on 8tracks",
    "version": "1.0",

    "background": {
        "scripts": [ "background.js" ]
    },

    "content_scripts": [{
        "matches": [ "http://8tracks.com/*" ],
        "js": [ "jquery-1.11.1.min.js", "content.js" ],
        "run_at": "document_start"
     }],

    "permissions": [
        "webNavigation", "http://8tracks.com/*"
    ]
}
